<h2>Ändrar <%= orgunit.namn %></h2><br/>

<form action="/orgunit/update/<%= orgunit.id %>" method="POST" class="form-horizontal">
  <div class="form-group">
    <label for="namn" class="col-sm-2">Namn</label>
    <div class="col-sm-10">
      <input type="text" value="<%= orgunit.namn %>" name="namn" class="form-control"/>
    </div>
  </div>

  <div class="form-group">
    <label for="orgnr" class="col-sm-2">Orgnr</label>
    <div class="col-sm-10">
      <input type="text" value="<%= orgunit.orgnr %>" name="orgnr" class="form-control"/>
    </div>
  </div>

  <br/>
  <div class="well">
    <input type="submit" value="Ändra" class="btn btn-lg btn-primary"/>
    <input type="hidden" name="_csrf" value="<%= _csrf %>"/>
  </div>
</form>